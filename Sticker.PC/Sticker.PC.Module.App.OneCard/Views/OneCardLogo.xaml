<UserControl x:Class="Sticker.PC.Module.App.OneCard.Views.OneCardLogo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sticker.PC.Module.App.OneCard.Views" 
             
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             
             mc:Ignorable="d" >
    <Grid>
        <Image Source="/Sticker.PC.Infra;component/Resources/Images/Global/background_main.png" Stretch="Fill" Tag="NO"/>
        <Image Source="/Sticker.PC.Infra;component/Resources/Images/OneCard/img_logo_background.png" Stretch="Fill" Tag="IN"/>
        <StackPanel Orientation="Vertical" VerticalAlignment="Bottom" HorizontalAlignment="Center">
            <Image Source="/Sticker.PC.Infra;component/Resources/Images/OneCard/img_logo_title.png" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform" Width="534" Height="209" Margin="0,0,0,29"/>
            <Image Source="/Sticker.PC.Infra;component/Resources/Images/OneCard/img_logo_cards.png" HorizontalAlignment="Center" VerticalAlignment="Bottom" Stretch="Uniform" Width="1292" Height="562"/>
        </StackPanel>
    </Grid>
</UserControl>
