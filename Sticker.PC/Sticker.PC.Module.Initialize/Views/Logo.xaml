             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sticker.PC.Module.Initialize.Views"
             x:Class="Sticker.PC.Module.Initialize.Views.Logo"
             mc:Ignorable="d" 
    
